      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ===== Helpers (compatíveis com Rhino) =====
function toStr(x) {
  return (x === null || x === undefined) ? "" : ("" + x).replace(/^\s+|\s+$/g, "");
}
function toTitle(s) {
  s = toStr(s).toLowerCase();
  var parts = s.split(/\s+/), i, w, out = [];
  for (i = 0; i &lt; parts.length; i++) {
    w = parts[i];
    if (!w) continue;
    out.push(w.charAt(0).toUpperCase() + w.substring(1));
  }
  return out.join(" ");
}
function parseIntSafe(x) {
  var s = toStr(x).replace(/[^\d-]/g, "");
  if (s === "" || s === "-") return null;
  var n = parseInt(s, 10);
  return isNaN(n) ? null : n;
}
function cleanUF(s) { return toStr(s).toUpperCase().replace(/[^A-Z]/g, ""); }
function ufOk(s)    { return /^[A-Z]{2}$/.test(s); }
function cleanCEP(s){ return toStr(s).replace(/\D/g, ""); }
function cepOk(s)   { return /^\d{8}$/.test(s); }

// ===== Entradas do CSV (com fallback de nomes) =====
// Esperado no CSV: id_pessoa;logradouro;numero;bairro;cidade;uf;cep
// Se você tiver renomeado em Select Values, coloque aqui alternativas.
var _id_pessoa  = (typeof id_pessoa  !== "undefined" &amp;&amp; id_pessoa  != null) ? parseIntSafe(id_pessoa)
                 : (typeof pessoa_id !== "undefined" &amp;&amp; pessoa_id != null) ? parseIntSafe(pessoa_id)
                 : null;

var _logradouro = (typeof logradouro !== "undefined" &amp;&amp; logradouro != null) ? toTitle(logradouro)
                 : (typeof end_logradouro_in !== "undefined" &amp;&amp; end_logradouro_in != null) ? toTitle(end_logradouro_in)
                 : "";

var _numeroInt  = (typeof numero !== "undefined" &amp;&amp; numero != null) ? parseIntSafe(numero)
                 : (typeof end_numero_in !== "undefined" &amp;&amp; end_numero_in != null) ? parseIntSafe(end_numero_in)
                 : null;

var _bairro     = (typeof bairro !== "undefined" &amp;&amp; bairro != null) ? toTitle(bairro)
                 : (typeof end_bairro_in !== "undefined" &amp;&amp; end_bairro_in != null) ? toTitle(end_bairro_in)
                 : "";

var _cidade     = (typeof cidade !== "undefined" &amp;&amp; cidade != null) ? toTitle(cidade)
                 : (typeof end_cidade_in !== "undefined" &amp;&amp; end_cidade_in != null) ? toTitle(end_cidade_in)
                 : "";

var _uf         = (typeof uf !== "undefined" &amp;&amp; uf != null) ? cleanUF(uf)
                 : (typeof end_uf_in !== "undefined" &amp;&amp; end_uf_in != null) ? cleanUF(end_uf_in)
                 : "";

var _cep        = (typeof cep !== "undefined" &amp;&amp; cep != null) ? cleanCEP(cep)
                 : (typeof end_cep_in !== "undefined" &amp;&amp; end_cep_in != null) ? cleanCEP(end_cep_in)
                 : "";

// ===== Validações =====
var msgs = [];
if (_id_pessoa === null || _id_pessoa &lt;= 0) msgs.push("id_pessoa inválido");
if (!_logradouro || _logradouro.length &lt; 3) msgs.push("logradouro inválido");
if (_numeroInt === null || _numeroInt &lt; 0) msgs.push("numero inválido");
if (!_cidade || _cidade.length &lt; 2) msgs.push("cidade inválida");
if (!ufOk(_uf)) msgs.push("UF inválida (2 letras)");
if (!cepOk(_cep)) msgs.push("CEP inválido (8 dígitos)");

// ===== Saídas do step (NÃO use var nas saídas) =====
// Campos finais normalizados
end_id_pessoa  = _id_pessoa;     // Integer
end_logradouro = _logradouro;    // String
end_numero     = _numeroInt;     // Integer
end_bairro     = _bairro;        // String
end_cidade     = _cidade;        // String
end_uf         = _uf;            // String(2)
end_cep        = _cep;           // String(8)
end_chave      = "" + _id_pessoa + "|" + _cep + "|" + _numeroInt; // String

// Status/erro com dois nomes (para não depender do nome no Filter Rows)
end_status     = (msgs.length === 0) ? "OK" : "ERRO";
end_erro       = (msgs.length === 0) ? "" : msgs.join("; ");

// >>> Duplicatas genéricas para roteamento (seu Filter Rows pode ler 'status'/'erro')
status         = end_status;
erro           = end_erro;

// (opcional) exposições para debug no arquivo de erros
src_id_pessoa  = toStr(id_pessoa);
src_logradouro = toStr(logradouro);
src_numero     = toStr(numero);
src_bairro     = toStr(bairro);
src_cidade     = toStr(cidade);
src_uf         = toStr(uf);
src_cep        = toStr(cep);
</jsScript_script>
      </jsScript>
