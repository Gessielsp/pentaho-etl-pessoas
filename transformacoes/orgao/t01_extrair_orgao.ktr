        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ===== Helpers =====
function toStr(x){ return (x===null || x===undefined) ? "" : String(x); }
function cleanSigla(s){ return toStr(s).toUpperCase().replace(/[^A-Z]/g,""); } // só A-Z
function siglaOk(s){ return /^[A-Z]{2,10}$/.test(s); }

function parseIntSafe(x){
  // aceita "2.692", "2,692", "2692", "2 692"
  var s = toStr(x).replace(/[^\d-]/g, ""); // mantém só dígitos e sinal
  if (s === "" || s === "-") return null;
  var n = parseInt(s, 10);
  return isNaN(n) ? null : n;
}
function servidoresOk(n){ return (n!==null) &amp;&amp; (n>=0) &amp;&amp; (n&lt;=1000000); }

// ===== Entradas (com fallback de nomes) =====
// CSV: id;sigla;servidores | Renomes possíveis: sigla_orgao, servidores_qty
var _sigla_in = (typeof sigla_orgao    !== "undefined" &amp;&amp; sigla_orgao    != null) ? sigla_orgao
              : (typeof sigla          !== "undefined" &amp;&amp; sigla          != null) ? sigla
              : "";

var _serv_in  = (typeof servidores_qty !== "undefined" &amp;&amp; servidores_qty != null) ? servidores_qty
              : (typeof servidores     !== "undefined" &amp;&amp; servidores     != null) ? servidores
              : "";

// Normalizações
var sigla_norm = cleanSigla(_sigla_in);
var serv_norm  = parseIntSafe(_serv_in);

// ===== Validações =====
var msgs = [];
if (!siglaOk(sigla_norm))     msgs.push("Sigla inválida (2-10 letras A-Z)");
if (!servidoresOk(serv_norm)) msgs.push("Servidores inválido (inteiro >= 0)");

// ===== Saídas (NÃO use var nas saídas) =====
orgao_sigla       = sigla_norm;
orgao_nome        = (sigla_norm ? sigla_norm : "DESCONHECIDO"); // evita NOT NULL
orgao_servidores  = (serv_norm === null) ? 0 : serv_norm;
orgao_chave       = sigla_norm;

// >>> Campos que o Filter Rows usa &lt;&lt;&lt;
status            = (msgs.length === 0) ? "OK" : "ERRO";
erro              = (msgs.length === 0) ? ""    : msgs.join("; ");

// (opcional) manter espelhado com prefixo orgao_
orgao_status      = status;
orgao_erro        = erro;

// (opcional) úteis para log/arquivo de erros
src_sigla         = toStr(_sigla_in);
src_servidores    = toStr(_serv_in);
</jsScript_script>
      </jsScript>
